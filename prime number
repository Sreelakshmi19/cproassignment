#include <stdio.h>
int main()
{
    int n,i;
    printf("Enter the number:\n");
    scanf("%d",&n);
    
    for(i=2; i<=n/2; i++)
    {
        if(n%i==0)
        {
            printf("Not a prime number\n");
            break;
        }
    }
    if(i==n/2+1)
        printf("It is a prime number\n");
    return 0;
}
